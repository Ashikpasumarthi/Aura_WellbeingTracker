{
  "name": "Aura- WellBeing Tracker",
  "version": "0.1.0",
  "description": "Notifying about user's well-being",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "idle",
    "storage",
    "tabs",
    "alarms",
    "scripting",
    "offscreen"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "welcome.html",
    "open_in_tab": true
  }
}